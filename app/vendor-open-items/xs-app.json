{
    "welcomeFile": "webapp/index.html",
    "authenticationMethod": "route",
    "routes": [
        {
            "source": "^/api/vendor/(.*)$",
            "target": "/api/vendor/$1",
            "destination": "srv-api",
            "authenticationType": "xsuaa",
            "csrfProtection": false
        },
        {
            "source": "^(.*)$",
            "target": "$1",
            "localDir": ".",
            "authenticationType": "xsuaa"
        }
    ]
}